{
  "package": "PMS.quickapp",
  "name": "PMS",
  "versionName": "1.0.0",
  "versionCode": 1,
  "minPlatformVersion": 1080,
  "icon": "/Common/assets/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.network"
    },
    {
      "name": "system.storage"
    },
    {
      "name": "system.fetch"
    },
    {
      "name": "service.health"
    },
    {
      "name": "system.resident"
    },
    {
      "name": "system.geolocation"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {
    "designWidth": 1080,
    "logLevel": "log",
    "background": {
      "features":[
        "system.geolocation"
      ]
    },
    "network": {
      "connectTimeout": 10000,
      "readTimeout": 10000,
      "writeTimeout": 10000
    }
  },
  "router": {
    "entry": "Index",
    "pages": {
      "Index": {
        "component": "index"
      },
      "Login": {
        "component": "form"
      },
      "Steps": {
        "component": "chart"
      },
      "ConsumptionList": {
        "component": "index"
      },
      "ConsumptionDetail": {
        "component": "form"
      },
      "IncomeList": {
        "component": "index"
      },
      "IncomeDetail": {
        "component": "form"
      },
      "DiaryList": {
        "component": "index"
      },
      "DiaryDetail": {
        "component": "form"
      },
      "NoteList": {
        "component": "index"
      },
      "NoteDetail": {
        "component": "form"
      },
      "PasswordList": {
        "component": "index"
      },
      "PasswordDetail": {
        "component": "form"
      }
    }
  },
  "display": {
    "themeMode": 1,
    "forceDark": false,
    "backgroundColor": "#000000",
    "fullScreen": false,
    "titleBar": false,
    "menu": false,
    "menuBarData": {
      "menuBar": false
    }
  }
}
