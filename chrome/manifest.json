{
  "name": "PMS",
  "description" : "PMS Chrome Extensions",
  "version": "0.1",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "https://api.pms.oonne.com/",
    "notifications",
    "bookmarks",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "/newtab/index.html"
  },
  "options_ui": {
    "page": "/options/index.html",
    "open_in_tab": true
  },
  "background": {
    "scripts": ["/background/index.js"]
  }
}